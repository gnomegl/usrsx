	kdrRe := regexp.MustCompile(`<div[^>]*class="[^"]*stat[^"]*"[^>]*>.*?K/D.*?([0-9.]+)</div>`)
	if matches := kdrRe.FindStringSubmatch(responseText); len(matches) > 1 {
